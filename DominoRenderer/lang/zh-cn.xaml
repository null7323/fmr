<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    
    <system:String x:Key="languageName">简体中文</system:String>

    <system:String x:Key="keyboardWidth">键盘宽度</system:String>
    <system:String x:Key="beatsOnScreen">屏幕节拍数</system:String>

    <system:String x:Key="previewWidth">预览窗口宽度</system:String>
    <system:String x:Key="previewHeight">预览窗口高度</system:String>

    <system:String x:Key="overrideColorSettings">覆盖颜色设置</system:String>
    <system:String x:Key="useProgramColors">使用主程序颜色</system:String>
    <system:String x:Key="useDominoColors">使用Domino颜色</system:String>

    <system:String x:Key="shuffleColor">随机颜色顺序</system:String>
    <system:String x:Key="restoreOrder">恢复颜色顺序</system:String>
    <system:String x:Key="onlyEffectiveWhenOverrideColors">只在使用Domino配色覆盖原设置时有效.</system:String>
</ResourceDictionary>